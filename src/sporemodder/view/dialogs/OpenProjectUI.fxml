<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="mainNode" prefHeight="491.0" prefWidth="403.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sporemodder.view.dialogs.OpenProjectUI">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <center>
      <ListView fx:id="list" fixedCellSize="30.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
      </ListView>
   </center>
   <bottom>
      <TextField fx:id="searchField" promptText="Search projects" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
