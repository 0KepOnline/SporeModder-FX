<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="mainNode" prefWidth="500.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sporemodder.view.dialogs.UnpackPresetsUI">
   <children>
      <Label fx:id="label" text="Presets are groups of .package files that SporeModder FX has considered that will be useful for modding. Using presets is recommended for better perfomance and easier mod development. Check in the following list all the presets you want to unpack (WARNING: this might take a few minutes)." textAlignment="JUSTIFY" wrapText="true">
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </Label>
      <VBox fx:id="presetsPane" spacing="10.0">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding></VBox>
      <TitledPane animated="false" maxHeight="250.0" prefHeight="250.0" prefWidth="294.0" text="Formats to convert">
         
         <content>
            <ScrollPane fitToHeight="true" fitToWidth="true">
               <content>
                  <VBox fx:id="settingsPane" prefHeight="200.0" prefWidth="100.0" spacing="10.0" >
                  <padding>
		            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
		         </padding>
                  </VBox>
               </content>
            </ScrollPane>
         </content>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="30.0" />
         </padding>
      </TitledPane>
   </children>
   <styleClass>
      <String fx:value="dialog-content" />
      <String fx:value="bigger-text" />
   </styleClass>
</VBox>
